{
  "Bot": {
    "Token": "",
    "DefaultLanguage": "ru-RU"
  },
  "Database": {
    "ConnectionString": "Data Source=hackathon.db"
  },
  "Modules": {
    "RoleModules": {
      "Admin": [ "AdminModule", "OrganizerModule", "ParticipantModule", "ViewerModule" ],
      "Organizer": [ "OrganizerModule", "ParticipantModule", "ViewerModule" ],
      "Participant": [ "ParticipantModule", "ViewerModule" ],
      "Viewer": [ "ViewerModule" ]
    }
  },
  "Localization": {
    "SupportedLanguages": [ "ru-RU", "en-US" ],
    "DefaultLanguage": "ru-RU",
    "CustomMessages": {
      "Welcome": {
        "ru-RU": "Добро пожаловать в бот для организации хакатона!",
        "en-US": "Welcome to the hackathon organization bot!"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}